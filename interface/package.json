    "@table-library/react-table-library": "4.1.15",
    "alova": "3.3.4",
    "async-validator": "^4.2.5",
    "formidable": "^3.5.4",
    "jwt-decode": "^4.0.0",
    "magic-string": "^0.30.18",
    "mime-types": "^3.0.1",
    "preact": "^10.27.1",
    "react": "^19.1.1",
